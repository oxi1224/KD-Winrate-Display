{
  "manifest_version": 3,
  "name": "Keydrop Winrate Display",
  "version": "2.1",
  "author": "oxi#6219",
  "icons": {
      "128": "icon.png" 
  },
  "description": "Pokazuje win rate'y wszystkich skrzynek!",
  "permissions": [
    "webRequest",
    "activeTab",
    "scripting"
  ],
  "action": {"default_icon":"icon.png"},
  "content_scripts": [{
      "matches": ["https://key-drop.com/*/*"],
      "js": ["index.js"],
      "run_at": "document_idle"
    }]
}